#!/bin/bash
if [ $(uname -s) = 'GNU/Linux' ]; then
	echo GNU/Linux
	exec emacsclient --alternate-editor="" -f /home/joe/.emacs.d/server/server  "$@"
elif [ $(uname -s) = 'Darwin' ]; then
	echo Darwin
	echo  "emacsclient --alternate-editor=\"\"  -f ~/.emacs.d/server/server \"$@\""
	exec emacsclient --alternate-editor=""  -f ~/.emacs.d/server/server "$@"
fi
